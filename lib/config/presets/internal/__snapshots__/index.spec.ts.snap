      "description": "\`apollo-server\` packages became scoped",
      "packageRules": [
        {
          "matchCurrentVersion": ">=3.10.3",
          "matchDatasources": [
            "npm",
          ],
          "matchPackageNames": [
            "apollo-server",
            "apollo-server-core",
            "apollo-server-express",
          ],
          "replacementName": "@apollo/server",
          "replacementVersion": "4.0.0",
        },
        {
          "matchCurrentVersion": ">=3.3.1",
          "matchDatasources": [
