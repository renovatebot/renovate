{
  "presets": [
    [
      "@babel/env",
      {
        "targets": {
          "node": true
        }
      }
    ],
    "@babel/typescript"
  ],
  "plugins": [
    "@babel/proposal-class-properties",
    "@babel/proposal-object-rest-spread"
  ],
  "sourceMaps": "inline",
  "retainLines": true,

  "env": {
    "test": {
      "plugins": ["dynamic-import-node"]
    }
  }
}
