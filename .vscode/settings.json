{
  "eslint.validate": [
    "javascript",
    { "language": "typescript", "autoFix": true }
  ],
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[typescript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[json]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "files.associations": {
    "Dockerfile.*": "dockerfile"
  }
}
